{
  "name": "npc-creator",
  "version": "1.0.0",
  "description": "Application for users to generate and save NPC names and descriptions",
  "main": "public/index.html",
  "scripts": {
    "start": "node server/index.js",
    "start-dev": "nodemon server/index.js",
    "heroku-postbuild": "npm install && npm run build",
    "build": "webpack",
    "watch": "webpack -w",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/austintestut/NPC-Creator.git"
  },
  "keywords": [
    "NPC",
    "Creator"
  ],
  "author": "Austin Testut",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/austintestut/NPC-Creator/issues"
  },
  "homepage": "https://github.com/austintestut/NPC-Creator#readme",
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/fileMock.js"
    }
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "axios": "^0.21.1",
    "cookie-session": "^1.4.0",
    "dotenv": "^9.0.0",
    "express": "^4.17.1",
    "nodemon": "^2.0.7",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "path": "^0.12.7",
    "postgres": "^1.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-components": "^5.2.2",
    "url-loader": "^4.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.13.1",
    "@babel/preset-env": "^7.13.5",
    "@babel/preset-react": "^7.12.13",
    "@testing-library/react": "^12.0.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.4.1",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "enzyme": "^3.11.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "react-test-renderer": "^17.0.2",
    "webpack": "^5.24.1",
    "webpack-cli": "^4.5.0"
  }
}
